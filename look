      function changeMessage(pCd, pTxt){
          var x=document.getElementById('opros_msg').rows
          if (pCd==1) { 
            var y=x[0].cells
            y[0].innerHTML=pTxt
            var y=x[1].cells
            y[0].innerHTML=""
          } else {
            var y=x[0].cells
            y[0].innerHTML=""
            var y=x[1].cells
            y[0].innerHTML=pTxt
          }
          

      }
      </SCRIPT>
<SCRIPT>
      function refresh_opros_page(fl) 
        {
          if (fl>1){
          var pURL = unescape(window.location.pathname)+"?p_fl="+fl;
          window.location.replace(pURL);
          }
        }    
