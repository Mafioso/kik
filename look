    <script>

        $(document).ready(function () {

        var b = $('#abtn');
        var c = $('#acont');
        b.toggle(function () {

            $("#abtn").toggleClass("stateVisible");
            $("#inputText").focus();
            b.animate({ 'margin-left': '-=397px', opacity: 1 }, 1000);
            c.animate({ 'left': '-=397px', opacity: 1 }, 1000);
        },
        function () {
            $("#abtn").toggleClass("stateVisible");
            $("#inputText").val('');
            b.animate({ 'margin-left': '+=397px', opacity: 1 }, 1000);
            c.animate({ 'left': '+=397px', opacity: 1 }, 1000);
        });




        $(function () {
            $(document).click(function (event) {
                if ($(event.target).closest("#acont").length) return;
                if ($(event.target).closest("#abtn").length) return;
                if ($("#abtn").attr("class") == "stateVisible") {
                    $('#abtn').trigger("click");
                }

            })
        });



        $.ajax({
            type: 'GET',
            url: '/portal/page/portal/mvd/MVD/mvd_panel_smi',
            success: function (data) {
                //alert("ok");
                $("#inner-content").html(data);

            },
            error: function () {

            }
        });
    });
    </script>


