 $(document).ready(function(){
       $('#rg458442 img').width(150);
       $('#rg458442 img').mouseover(function()
       {
          $(this).css("cursor","pointer");
          $(this).animate({width: "400px", height : "250px"}, 'normal');
       });
    
    $('#rg458442 img').mouseout(function()
      {   
          $(this).animate({width: "150px", height: "88px"}, 'normal');
       });
   });
